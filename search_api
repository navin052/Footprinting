# Python
import requests
import os

# Load API key and Search Engine ID securely
API_KEY = os.getenv("GOOGLE_API_KEY")       # Store API key as an environment variable
SEARCH_ENGINE_ID = os.getenv("GOOGLE_CSE_ID")  # Store Custom Search Engine ID

def google_search(query, api_key, cse_id, num=10):
    """Perform a Google Custom Search and return results."""
    url = "https://www.googleapis.com/customsearch/v1"
    params = {
        "q": query,
        "key": api_key,
        "cx": cse_id,
        "num": num  # max 10 per request
    }

    try:
        response = requests.get(url, params=params, timeout=10)
        response.raise_for_status()
        results = response.json()

        if "items" in results:
            return results["items"]
        else:
            print("No results found.")
            return []

    except requests.RequestException as e:
        print(f"Error fetching data: {e}")
        return []

if __name__ == "__main__":
    query = "inurl:admin login"
    results = google_search(query, API_KEY, SEARCH_ENGINE_ID)

    # Print results
    for item in results:
        print(f"Title: {item.get('title')}")
        print(f"Link: {item.get('link')}")
        print(f"Snippet: {item.get('snippet')}\n")
